{
    "name": "www.giladpeleg.com",
    "private": true,
    "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
    "version": "1.0.0",
    "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
    "repository": "pgilad/www.giladpeleg.com",
    "dependencies": {
        "eslint-plugin-flowtype": "8.0.3",
        "gatsby": "5.12.8",
        "gatsby-cli": "5.12.3",
        "gatsby-plugin-canonical-urls": "5.12.0",
        "gatsby-plugin-catch-links": "5.12.0",
        "gatsby-plugin-cloudflare-pages": "^1.0.0-alpha.2",
        "gatsby-plugin-feed": "5.12.2",
        "gatsby-plugin-google-gtag": "5.12.2",
        "gatsby-plugin-image": "3.12.2",
        "gatsby-plugin-manifest": "5.12.2",
        "gatsby-plugin-netlify": "5.1.1",
        "gatsby-plugin-offline": "6.12.2",
        "gatsby-plugin-robots-txt": "1.8.0",
        "gatsby-plugin-sharp": "5.12.2",
        "gatsby-plugin-sitemap": "6.12.2",
        "gatsby-remark-autolink-headers": "6.12.2",
        "gatsby-remark-copy-linked-files": "6.12.0",
        "gatsby-remark-embed-gist": "1.2.1",
        "gatsby-remark-emojis": "0.4.3",
        "gatsby-remark-images": "7.12.2",
        "gatsby-remark-prismjs": "7.12.0",
        "gatsby-source-filesystem": "5.12.0",
        "gatsby-transformer-remark": "6.12.2",
        "gatsby-transformer-sharp": "5.12.2",
        "postcss": "^8.4.31",
        "prism-themes": "1.9.0",
        "prismjs": "1.29.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "5.2.2"
    },
    "keywords": [
        "blog",
        "personal",
        "technology",
        "writings",
        "creative",
        "developer",
        "thoughts",
        "manager",
        "gilad",
        "peleg",
        "typescript",
        "static",
        "gatsby"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "eslint": "eslint --fix .",
        "format": "prettier --write '{src,scripts}/**/*.{js,css,ts,tsx}' '*.{js,ts,tsx}'",
        "format-check": "prettier --check '{src,scripts}/**/*.{js,css,ts,tsx}' '*.{js,ts,tsx}'",
        "lint": "npm run eslint && npm run stylelint",
        "release-prepare": "npm run build",
        "release:dev": "NODE_ENV=staging npm run release-prepare && netlify deploy",
        "release:prod": "NODE_ENV=production npm run release-prepare && netlify deploy --prod",
        "start": "npm run develop",
        "stylelint": "stylelint 'src/**/*.css' --fix",
        "typecheck": "tsc --project . --noEmit",
        "verify": "npm run format-check && npm run lint && npm run typecheck"
    },
    "devDependencies": {
        "@eslint/js": "8.52.0",
        "@types/chai": "^4.3.9",
        "@types/eslint": "8.44.6",
        "@types/mocha": "^10.0.3",
        "@types/node": "20.8.7",
        "@types/prettier": "2.7.3",
        "@types/prismjs": "1.26.2",
        "@types/react": "18.2.31",
        "@types/react-dom": "18.2.14",
        "@types/source-map-support": "0.5.9",
        "@types/stylelint": "13.13.3",
        "@types/typography": "0.16.6",
        "@typescript-eslint/eslint-plugin": "6.8.0",
        "@typescript-eslint/parser": "6.8.0",
        "axios": "1.5.1",
        "chai": "4.3.10",
        "csp-builder": "1.1.1",
        "eslint": "8.52.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "globals": "13.23.0",
        "mocha": "10.2.0",
        "netlify-cli": "16.9.1",
        "prettier": "3.0.3",
        "source-map-support": "0.5.21",
        "stylelint": "15.11.0",
        "stylelint-config-recommended": "13.0.0",
        "stylelint-order": "6.0.3",
        "ts-node": "^10.9.1"
    },
    "browserslist": [
        ">1%",
        "not dead",
        "not op_mini all",
        "not ie <= 11",
        "not safari <= 12"
    ]
}
