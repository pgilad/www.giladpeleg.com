{
    "name": "www.giladpeleg.com",
    "private": true,
    "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
    "version": "1.0.0",
    "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
    "repo": "pgilad/www.giladpeleg.com",
    "dependencies": {
        "gatsby": "2.22.11",
        "gatsby-cli": "2.12.36",
        "gatsby-image": "2.4.5",
        "gatsby-plugin-canonical-urls": "2.3.2",
        "gatsby-plugin-catch-links": "2.3.3",
        "gatsby-plugin-feed": "2.5.3",
        "gatsby-plugin-google-analytics": "2.3.2",
        "gatsby-plugin-manifest": "2.4.9",
        "gatsby-plugin-netlify": "2.3.3",
        "gatsby-plugin-offline": "3.2.7",
        "gatsby-plugin-react-helmet": "3.3.2",
        "gatsby-plugin-robots-txt": "1.5.1",
        "gatsby-plugin-sharp": "2.6.9",
        "gatsby-plugin-sitemap": "2.4.3",
        "gatsby-plugin-typescript": "2.4.3",
        "gatsby-remark-autolink-headers": "2.3.3",
        "gatsby-remark-copy-linked-files": "2.3.3",
        "gatsby-remark-embed-gist": "1.1.9",
        "gatsby-remark-emojis": "0.4.3",
        "gatsby-remark-images": "3.3.8",
        "gatsby-remark-prismjs": "3.5.2",
        "gatsby-source-filesystem": "2.3.8",
        "gatsby-transformer-remark": "2.8.13",
        "gatsby-transformer-sharp": "2.5.3",
        "prism-themes": "1.4.0",
        "prismjs": "1.20.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-helmet": "6.0.0",
        "typescript": "3.9.3"
    },
    "keywords": [
        "blog",
        "personal",
        "technology",
        "writings",
        "creative",
        "developer",
        "thoughts",
        "manager",
        "gilad",
        "peleg",
        "typescript",
        "static",
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "codegen": "gql-gen --config codegen.yml",
        "develop": "gatsby develop",
        "eslint": "eslint . --fix",
        "format": "prettier --write '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "format-check": "prettier --check '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "lint": "npm run eslint && npm run stylelint",
        "release-prepare": "npm run build",
        "release:dev": "GATSBY_ENV=staging npm run release-prepare && netlify deploy",
        "release:prod": "GATSBY_ENV=production npm run release-prepare && netlify deploy --prod",
        "start": "npm run develop",
        "stylelint": "stylelint 'src/**/*.css' --fix",
        "typecheck": "tsc --project . --noEmit",
        "verify": "npm run format-check && npm run lint && npm run typecheck"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.15.0",
        "@graphql-codegen/typescript": "1.15.0",
        "@graphql-codegen/typescript-operations": "1.15.0",
        "@types/eslint": "6.8.1",
        "@types/node": "14.0.5",
        "@types/prettier": "2.0.1",
        "@types/prismjs": "1.16.1",
        "@types/react": "16.9.35",
        "@types/react-dom": "16.9.8",
        "@types/react-helmet": "6.0.0",
        "@types/source-map-support": "0.5.1",
        "@types/stylelint": "^9.10.1",
        "@types/typography": "^0.16.3",
        "@typescript-eslint/eslint-plugin": "3.0.2",
        "@typescript-eslint/parser": "3.0.2",
        "csp-builder": "^1.0.1",
        "eslint": "7.1.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-prettier": "3.1.3",
        "eslint-plugin-react": "7.20.0",
        "netlify-cli": "2.52.0",
        "prettier": "2.0.5",
        "source-map-support": "0.5.19",
        "stylelint": "13.5.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-order": "4.0.0",
        "ts-node": "8.10.1"
    }
}
