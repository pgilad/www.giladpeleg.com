{
    "name": "www.giladpeleg.com",
    "private": true,
    "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
    "version": "1.0.0",
    "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
    "repo": "pgilad/www.giladpeleg.com",
    "dependencies": {
        "gatsby": "2.15.13",
        "gatsby-cli": "2.7.47",
        "gatsby-image": "2.2.18",
        "gatsby-plugin-canonical-urls": "2.1.7",
        "gatsby-plugin-catch-links": "2.1.9",
        "gatsby-plugin-feed": "2.3.12",
        "gatsby-plugin-google-analytics": "2.1.16",
        "gatsby-plugin-manifest": "2.2.16",
        "gatsby-plugin-netlify": "2.1.12",
        "gatsby-plugin-offline": "3.0.6",
        "gatsby-plugin-react-helmet": "3.1.7",
        "gatsby-plugin-robots-txt": "1.5.0",
        "gatsby-plugin-sharp": "2.2.21",
        "gatsby-plugin-sitemap": "2.2.12",
        "gatsby-plugin-typescript": "2.1.7",
        "gatsby-remark-autolink-headers": "2.1.10",
        "gatsby-remark-copy-linked-files": "2.1.18",
        "gatsby-remark-embed-gist": "1.1.9",
        "gatsby-remark-emojis": "0.3.2",
        "gatsby-remark-images": "3.1.21",
        "gatsby-remark-prismjs": "3.3.13",
        "gatsby-source-filesystem": "2.1.22",
        "gatsby-transformer-remark": "2.6.22",
        "gatsby-transformer-sharp": "2.2.14",
        "prism-themes": "1.1.0",
        "prismjs": "1.17.1",
        "react": "16.9.0",
        "react-dom": "16.9.0",
        "react-helmet": "5.2.1",
        "typescript": "3.6.2"
    },
    "keywords": [
        "blog",
        "personal",
        "technology",
        "writings",
        "creative",
        "developer",
        "thoughts",
        "manager",
        "gilad",
        "peleg",
        "typescript",
        "static",
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "codegen": "gql-gen --config codegen.yml",
        "develop": "gatsby develop",
        "eslint": "eslint . --fix",
        "format": "prettier --write '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "format-check": "prettier --check '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "lint": "npm run eslint && npm run stylelint",
        "release-prepare": "gatsby clean && npm run build",
        "release:dev": "GATSBY_ENV=staging npm run release-prepare && netlify deploy",
        "release:prod": "GATSBY_ENV=production npm run release-prepare && netlify deploy --prod",
        "start": "npm run develop",
        "stylelint": "stylelint 'src/**/*.css' --fix",
        "typecheck": "tsc --project . --noEmit",
        "verify": "npm run format-check && npm run lint && npm run typecheck"
    },
    "devDependencies": {
        "@types/eslint": "^6.1.1",
        "@types/node": "^12.7.4",
        "@types/prettier": "^1.18.2",
        "@types/prismjs": "^1.16.0",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/react-helmet": "^5.0.9",
        "@types/source-map-support": "^0.5.0",
        "@types/stylelint": "^9.10.1",
        "@types/typography": "^0.16.3",
        "@typescript-eslint/eslint-plugin": "^2.2.0",
        "@typescript-eslint/parser": "^2.2.0",
        "csp-builder": "^1.0.1",
        "eslint": "^6.3.0",
        "eslint-config-prettier": "^6.2.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "graphql-code-generator": "^0.18.2",
        "graphql-codegen-typescript-common": "^0.18.2",
        "graphql-codegen-typescript-server": "0.18.2",
        "netlify-cli": "^2.13.1",
        "prettier": "^1.18.2",
        "source-map-support": "^0.5.13",
        "stylelint": "^10.1.0",
        "stylelint-config-recommended": "^2.2.0",
        "stylelint-order": "^3.1.0",
        "ts-node": "^8.3.0"
    }
}
