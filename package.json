{
  "name": "gilad-peleg-website",
  "private": true,
  "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
  "version": "1.0.0",
  "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
  "repo": "pgilad/www.giladpeleg.com",
  "dependencies": {
    "gatsby": "^2.3.16",
    "gatsby-cli": "^2.5.6",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-canonical-urls": "^2.0.12",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-feed": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.28",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.33",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-plugin-typescript": "^2.0.12",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-embed-gist": "^1.1.7",
    "gatsby-remark-emojis": "^0.3.2",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.8",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "prism-themes": "^1.1.0",
    "prismjs": "PrismJS/prism#master",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "typescript": "^3.4.3"
  },
  "keywords": [
    "blog",
    "personal",
    "technology",
    "writings",
    "creative",
    "developer",
    "thoughts",
    "manager",
    "gilad",
    "peleg",
    "typescript",
    "static",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "codegen": "gql-gen --config codegen.yml",
    "develop": "gatsby develop",
    "eslint": "eslint . --fix",
    "format": "prettier --write '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
    "lint": "npm run eslint && npm run tslint && npm run stylelint",
    "release-prepare": "gatsby clean && npm run build",
    "release:dev": "GATSBY_ENV=staging npm run release-prepare && netlify deploy",
    "release:prod": "GATSBY_ENV=production npm run release-prepare && netlify deploy --prod",
    "start": "npm run develop",
    "stylelint": "stylelint 'src/**/*.css' --fix",
    "tslint": "tslint '{src,gatsby}/**/*.{ts,tsx}' --fix",
    "typecheck": "tsc --project . --noEmit",
    "verify": "npm run format && npm run lint && npm run stylelint && npm run typecheck"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/node": "^11.13.2",
    "@types/prettier": "^1.16.1",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.8",
    "@types/source-map-support": "^0.5.0",
    "@types/stylelint": "^9.10.0",
    "@types/typography": "^0.16.2",
    "babel-eslint": "^10.0.1",
    "csp-builder": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "graphql-code-generator": "^0.18.0",
    "graphql-codegen-typescript-common": "^0.18.0",
    "graphql-codegen-typescript-server": "0.18.0",
    "netlify-cli": "^2.11.4",
    "prettier": "^1.16.4",
    "source-map-support": "^0.5.12",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^2.2.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  }
}
