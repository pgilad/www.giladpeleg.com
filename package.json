{
  "name": "gilad-peleg-website",
  "private": true,
  "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
  "version": "1.0.0",
  "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
  "repo": "pgilad/personal-site",
  "dependencies": {
    "disqus-react": "^1.0.5",
    "gatsby": "^2.1.18",
    "gatsby-cli": "^2.4.11",
    "gatsby-image": "^2.0.30",
    "gatsby-plugin-canonical-urls": "^2.0.10",
    "gatsby-plugin-catch-links": "^2.0.11",
    "gatsby-plugin-feed": "^2.0.14",
    "gatsby-plugin-google-analytics": "^2.0.14",
    "gatsby-plugin-manifest": "^2.0.19",
    "gatsby-plugin-netlify": "^2.0.11",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.22",
    "gatsby-plugin-sitemap": "^2.0.6",
    "gatsby-plugin-typescript": "^2.0.9",
    "gatsby-remark-autolink-headers": "^2.0.14",
    "gatsby-remark-copy-linked-files": "^2.0.9",
    "gatsby-remark-images": "^3.0.5",
    "gatsby-remark-prismjs": "^3.2.4",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.0",
    "gatsby-transformer-sharp": "^2.1.14",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "typescript": "^3.3.3333"
  },
  "keywords": [
    "blog",
    "personal",
    "technology",
    "writings",
    "creative",
    "developer",
    "thoughts",
    "manager",
    "gilad",
    "peleg",
    "typescript",
    "static",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write 'src/**/*.{js,css,ts,tsx}' '*.js'",
    "lint": "npm run eslint && npm run tslint && npm run stylelint",
    "typecheck": "tsc --project . --noEmit",
    "eslint": "eslint . --fix",
    "tslint": "tslint 'src/**/*.{ts,tsx}' --fix",
    "stylelint": "stylelint 'src/**/*.css' --fix",
    "codegen": "gql-gen --config codegen.yml",
    "prerelease:dev": "rm -rf public .cache && npm run build",
    "prerelease:prod": "npm run prerelease:dev",
    "release:dev": "netlify deploy",
    "release:prod": "netlify deploy --prod",
    "verify": "npm run format && npm run lint && npm run stylelint && npm run typecheck"
  },
  "devDependencies": {
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-helmet": "^5.0.8",
    "@types/typography": "^0.16.2",
    "babel-eslint": "^10.0.1",
    "csp-builder": "^1.0.1",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "graphql-code-generator": "^0.17.0",
    "graphql-codegen-typescript-common": "^0.17.0",
    "graphql-codegen-typescript-server": "0.17.0",
    "netlify-cli": "^2.8.1",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^2.0.0",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0"
  }
}
