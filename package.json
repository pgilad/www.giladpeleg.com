        "mocha": "10.4.0",
        "prettier": "3.3.0",
        "source-map-support": "0.5.21",
        "stylelint": "16.6.1",
        "stylelint-config-recommended": "14.0.0",
        "stylelint-order": "6.0.4",
        "ts-node": "10.9.2",
