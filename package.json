{
  "name": "gilad-peleg-website",
  "private": true,
  "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
  "version": "1.0.0",
  "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
  "repo": "pgilad/www.giladpeleg.com",
  "dependencies": {
    "gatsby": "2.5.5",
    "gatsby-cli": "2.5.15",
    "gatsby-image": "2.1.0",
    "gatsby-plugin-canonical-urls": "2.0.13",
    "gatsby-plugin-catch-links": "2.0.14",
    "gatsby-plugin-feed": "2.2.0",
    "gatsby-plugin-google-analytics": "2.0.20",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.17",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-robots-txt": "1.4.0",
    "gatsby-plugin-sharp": "2.0.37",
    "gatsby-plugin-sitemap": "2.1.0",
    "gatsby-plugin-typescript": "2.0.14",
    "gatsby-remark-autolink-headers": "2.0.16",
    "gatsby-remark-copy-linked-files": "2.0.12",
    "gatsby-remark-embed-gist": "1.1.7",
    "gatsby-remark-emojis": "0.3.2",
    "gatsby-remark-images": "3.0.11",
    "gatsby-remark-prismjs": "3.2.9",
    "gatsby-source-filesystem": "2.0.36",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.19",
    "prism-themes": "1.1.0",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "typescript": "3.4.5"
