{
    "name": "www.giladpeleg.com",
    "private": true,
    "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
    "version": "1.0.0",
    "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
    "repo": "pgilad/www.giladpeleg.com",
    "dependencies": {
        "gatsby": "2.24.23",
        "gatsby-cli": "2.12.68",
        "gatsby-image": "2.4.14",
        "gatsby-plugin-canonical-urls": "2.3.10",
        "gatsby-plugin-catch-links": "2.3.11",
        "gatsby-plugin-feed": "2.5.11",
        "gatsby-plugin-google-analytics": "2.3.13",
        "gatsby-plugin-graphql-codegen": "2.7.1",
        "gatsby-plugin-manifest": "2.4.21",
        "gatsby-plugin-netlify": "2.3.11",
        "gatsby-plugin-offline": "3.2.21",
        "gatsby-plugin-react-helmet": "3.3.10",
        "gatsby-plugin-robots-txt": "1.5.1",
        "gatsby-plugin-sharp": "2.6.24",
        "gatsby-plugin-sitemap": "2.4.11",
        "gatsby-plugin-typescript": "2.4.16",
        "gatsby-remark-autolink-headers": "2.3.11",
        "gatsby-remark-copy-linked-files": "2.3.12",
        "gatsby-remark-embed-gist": "1.2.0",
        "gatsby-remark-emojis": "0.4.3",
        "gatsby-remark-images": "3.3.23",
        "gatsby-remark-prismjs": "3.5.10",
        "gatsby-source-filesystem": "2.3.23",
        "gatsby-transformer-remark": "2.8.27",
        "gatsby-transformer-sharp": "2.5.12",
        "prism-themes": "1.4.0",
        "prismjs": "1.20.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-helmet": "6.1.0",
        "typescript": "3.9.7"
    },
    "keywords": [
        "blog",
        "personal",
        "technology",
        "writings",
        "creative",
        "developer",
        "thoughts",
        "manager",
        "gilad",
        "peleg",
        "typescript",
        "static",
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "codegen": "gql-gen --config codegen.yml",
        "develop": "gatsby develop",
        "eslint": "eslint . --fix",
        "format": "prettier --write '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "format-check": "prettier --check '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "lint": "npm run eslint && npm run stylelint",
        "release-prepare": "npm run build",
        "release:dev": "GATSBY_ENV=staging npm run release-prepare && netlify deploy",
        "release:prod": "GATSBY_ENV=production npm run release-prepare && netlify deploy --prod",
        "start": "npm run develop",
        "stylelint": "stylelint 'src/**/*.css' --fix",
        "typecheck": "tsc --project . --noEmit",
        "verify": "npm run format-check && npm run lint && npm run typecheck"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.17.7",
        "@graphql-codegen/typescript": "1.17.7",
        "@graphql-codegen/typescript-operations": "1.17.7",
        "@types/eslint": "7.2.0",
        "@types/node": "14.0.27",
        "@types/prettier": "2.0.2",
        "@types/prismjs": "1.16.1",
        "@types/react": "16.9.44",
        "@types/react-dom": "16.9.8",
        "@types/react-helmet": "6.0.0",
        "@types/source-map-support": "0.5.2",
        "@types/stylelint": "^9.10.1",
        "@types/typography": "^0.16.3",
        "@typescript-eslint/eslint-plugin": "3.7.1",
        "@typescript-eslint/parser": "3.7.1",
        "csp-builder": "^1.0.1",
        "eslint": "7.6.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.20.5",
        "netlify-cli": "2.58.0",
        "prettier": "2.0.5",
        "source-map-support": "0.5.19",
        "stylelint": "13.6.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-order": "4.1.0",
        "ts-node": "8.10.2"
    }
}
