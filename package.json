        "gilad",
        "peleg",
        "typescript",
        "static",
        "gatsby"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "eslint": "eslint --fix .",
        "format": "prettier --write '{src,scripts,test}/**/*.{js,css,ts,tsx}' '*.{js,ts,tsx}'",
        "format-check": "prettier --check '{src,scripts,test}/**/*.{js,css,ts,tsx}' '*.{js,ts,tsx}'",
        "lint": "npm run eslint && npm run stylelint",
        "release-prepare": "npm run build",
        "release:dev": "NODE_ENV=staging npm run release-prepare",
        "release:prod": "NODE_ENV=production npm run release-prepare",
        "start": "npm run develop",
        "stylelint": "stylelint 'src/**/*.css' --fix",
        "typecheck": "tsc --project . --noEmit",
        "verify": "npm run format-check && npm run lint && npm run typecheck"
    },
    "devDependencies": {
        "@eslint/js": "8.56.0",
        "@types/chai": "4.3.16",
        "@types/eslint": "8.56.10",
        "@types/mocha": "10.0.6",
        "@types/node": "20.13.0",
        "@types/prismjs": "1.26.4",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/source-map-support": "0.5.10",
        "@types/typography": "0.16.7",
        "@typescript-eslint/eslint-plugin": "7.11.0",
        "@typescript-eslint/parser": "7.11.0",
        "axios": "1.7.2",
        "chai": "5.1.1",
        "csp-builder": "1.1.1",
