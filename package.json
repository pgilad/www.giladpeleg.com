{
    "name": "www.giladpeleg.com",
    "private": true,
    "description": "My personal blog, where I write about technology, development methodologies and general thoughts about life",
    "version": "1.0.0",
    "author": "Gilad Peleg <gilad@giladpeleg.com> (https://www.giladpeleg.com)",
    "repository": "pgilad/www.giladpeleg.com",
    "dependencies": {
        "gatsby": "2.25.1",
        "gatsby-cli": "2.12.115",
        "gatsby-image": "2.4.21",
        "gatsby-plugin-canonical-urls": "2.3.13",
        "gatsby-plugin-catch-links": "2.3.15",
        "gatsby-plugin-feed": "2.6.0",
        "gatsby-plugin-google-analytics": "2.4.0",
        "gatsby-plugin-google-gtag": "^2.1.13",
        "gatsby-plugin-graphql-codegen": "2.7.1",
        "gatsby-plugin-manifest": "2.5.1",
        "gatsby-plugin-netlify": "2.4.0",
        "gatsby-plugin-offline": "3.3.1",
        "gatsby-plugin-react-helmet": "3.3.14",
        "gatsby-plugin-robots-txt": "1.5.3",
        "gatsby-plugin-sharp": "2.7.0",
        "gatsby-plugin-sitemap": "2.5.0",
        "gatsby-plugin-typescript": "2.5.0",
        "gatsby-remark-autolink-headers": "2.4.0",
        "gatsby-remark-copy-linked-files": "2.3.19",
        "gatsby-remark-embed-gist": "1.2.1",
        "gatsby-remark-emojis": "0.4.3",
        "gatsby-remark-images": "3.4.0",
        "gatsby-remark-prismjs": "3.6.0",
        "gatsby-source-filesystem": "2.4.0",
        "gatsby-transformer-remark": "2.9.1",
        "gatsby-transformer-sharp": "2.5.21",
        "prism-themes": "1.5.0",
        "prismjs": "1.22.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet": "6.1.0",
        "typescript": "4.0.5"
    },
    "keywords": [
        "blog",
        "personal",
        "technology",
        "writings",
        "creative",
        "developer",
        "thoughts",
        "manager",
        "gilad",
        "peleg",
        "typescript",
        "static",
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "codegen": "gql-gen --config codegen.yml",
        "develop": "gatsby develop",
        "eslint": "eslint . --fix",
        "format": "prettier --write '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "format-check": "prettier --check '{src,gatsby}/**/*.{js,css,ts,tsx}' '*.js'",
        "lint": "npm run eslint && npm run stylelint",
        "release-prepare": "npm run build",
        "release:dev": "GATSBY_ENV=staging npm run release-prepare && netlify deploy",
        "release:prod": "GATSBY_ENV=production npm run release-prepare && netlify deploy --prod",
        "start": "npm run develop",
        "stylelint": "stylelint 'src/**/*.css' --fix",
        "typecheck": "tsc --project . --noEmit",
        "verify": "npm run format-check && npm run lint && npm run typecheck"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.19.0",
        "@graphql-codegen/typescript": "1.17.11",
        "@graphql-codegen/typescript-operations": "1.17.8",
        "@types/eslint": "7.2.4",
        "@types/node": "14.14.6",
        "@types/prettier": "2.1.5",
        "@types/prismjs": "1.16.2",
        "@types/react": "16.9.55",
        "@types/react-dom": "16.9.9",
        "@types/react-helmet": "6.1.0",
        "@types/source-map-support": "0.5.3",
        "@types/stylelint": "^9.10.1",
        "@types/typography": "^0.16.3",
        "@typescript-eslint/eslint-plugin": "4.6.1",
        "@typescript-eslint/parser": "4.6.1",
        "csp-builder": "^1.1.1",
        "eslint": "7.12.1",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.21.5",
        "netlify-cli": "2.67.2",
        "prettier": "2.1.2",
        "source-map-support": "0.5.19",
        "stylelint": "13.7.2",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-order": "4.1.0",
        "ts-node": "9.0.0"
    }
}
